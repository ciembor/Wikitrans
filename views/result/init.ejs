<h2>results</h2>
<table cellspacing='0px'>
	<thead>
	<tr>
		<% for(var attr in Wikitrans.Models.Result.attributes){%>
			<% if(attr == 'id') continue;%>
			<th><%= attr%> </th>	 
		<%}%>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= $.View('//wikitrans/views/result/list',{results: results})%>
	</tbody>
</table>
<h2>New result</h2>
<form>
	<% for(var attr in Wikitrans.Models.Result.attributes){ %>
		<% if(attr == 'id') continue;%>
		<div>
			<label><%= attr %> </label><br />
			<input type="text" value="" name="<%= attr %>"/>
		</div>
	<%}%>
	<input type='submit' value='Create'/>
</form>